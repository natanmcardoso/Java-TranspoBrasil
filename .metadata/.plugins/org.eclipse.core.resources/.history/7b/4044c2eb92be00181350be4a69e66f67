package br.com.cursojava.aula002;
import java.util.Scanner;

public class Exercicio12 {
	public static void main(String [] args){
		System.out.println("Calculo Média. \n");
		
		Scanner media = new Scanner(System.in);
		System.out.printf("Informe a 1º nota: \n");
		double nota1 = Double.parseDouble(media.nextLine());
		
		System.out.printf("Informe a 1º nota: \n");
		double nota2 = Double.parseDouble(media.nextLine());
		
		System.out.printf("Informe a 1º nota: \n");
		double nota3 = Double.parseDouble(media.nextLine());
		
		System.out.printf("Informe a 1º nota: \n");
		double nota4 = Double.parseDouble(media.nextLine());
		
		double nota5 = Double.parseDouble(nota1);
		double nota6 = Double.parseDouble(nota2);
		double nota7 = Double.parseDouble(nota3);
		double nota8 = Double.parseDouble(nota4);
		
	}
}
